<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/idscrollview3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".miPerfil">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="41dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="-47dp"
        tools:layout_editor_absoluteY="42dp">

        <ImageView
            android:id="@+id/igmpst"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/top_background2" />

        <TextView
            android:id="@+id/textView6ww"
            android:layout_width="375dp"
            android:layout_height="91dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/postlog"
            android:textAlignment="textStart"
            android:textColor="#25523D"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView12qq"
            android:layout_width="130dp"
            android:layout_height="125dp"
            android:src="@drawable/hats_logo_1_"
            app:layout_constraintBottom_toBottomOf="@+id/textView6ww"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />


        <TextView
            android:id="@+id/textAnclajeEditar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="900dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.464"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/lblNombreMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="160dp"
            android:background="@drawable/edittextfondo"
            android:backgroundTint="#FFFFFF"
            android:fontFamily="sans-serif-medium"
            android:text="Nombre"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblApellidoMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/edittextfondo"
            android:fontFamily="sans-serif-medium"
            android:text="Apellido"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblNombreMP" />

        <TextView
            android:id="@+id/lblCorreoMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/edittextfondo"
            android:fontFamily="sans-serif-medium"
            android:text="Correo"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblApellidoMP" />

        <TextView
            android:id="@+id/lblNumeroMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/edittextfondo"
            android:fontFamily="sans-serif-medium"
            android:text="Numero"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblCorreoMP" />

        <TextView
            android:id="@+id/lblFechaMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/edittextfondo"
            android:fontFamily="sans-serif-medium"
            android:text="Fecha de nacimiento"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblNumeroMP" />

        <Button
            android:id="@+id/btnEditarMPP"
            android:layout_width="243dp"
            android:layout_height="53dp"

            android:layout_marginTop="20dp"
            android:backgroundTint="#417655"
            android:text="Editar mi perfil"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblDuiMP"
            app:layout_constraintVertical_bias="0.142"
            app:rippleColor="#FFFFFF"

            />


        <TextView
            android:id="@+id/lblDuiMP"
            android:layout_width="286dp"
            android:layout_height="41dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/edittextfondo"
            android:fontFamily="sans-serif-medium"
            android:text="Dui"
            android:textAlignment="center"
            android:textColor="#5B8166"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblFechaMP" />


        <TextView
            android:id="@+id/textView6"
            android:layout_width="375dp"
            android:layout_height="91dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/postlog"
            android:textAlignment="textStart"
            android:textColor="#25523D"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView13d6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="Perfil"
            android:textColor="#669469"
            android:textSize="34sp"
            android:textStyle="bold"
            app:cornerRadius="15dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.395"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintTop_toTopOf="@+id/textView6" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="130dp"
            android:layout_height="125dp"
            android:src="@drawable/hats_logo_1_"
            app:layout_constraintBottom_toBottomOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>